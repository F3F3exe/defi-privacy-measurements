/* General */
html {
	font-size: 1vw;
}
.af-view {
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.af-view a {
	color: inherit;
}
@media only screen and (max-width: 767px) {
	.af-view a {
		cursor: default !important;
	}
}
.af-view * {
    -webkit-tap-highlight-color: transparent;
}
.af-view *:focus {
    outline: 0 !important;
}
.af-view input {
	-webkit-appearance: none;
	-webkit-border-radius: 0;
}
.af-view textarea {
    border: none;
    overflow: auto;
    outline: none;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    resize: none; /*remove the resize handle on the bottom right*/
}
input:-webkit-autofill,
input:-webkit-autofill:hover,
input:-webkit-autofill:focus,
input:-webkit-autofill:active {
    -webkit-transition: "color 9999s ease-out, background-color 9999s ease-out";
    -webkit-transition-delay: 9999s;
}
.input-field:hover::placeholder{
	opacity: 0.5;
}
.input-field:focus::placeholder{
	opacity: 0.5;
}
/* Chrome, Safari, Edge, Opera */
.af-view input::-webkit-outer-spin-button,
.af-view input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
/* Firefox */
.af-view input[type=number] {
  -moz-appearance: textfield;
}
.af-view .af-class-disable-slider-button {
	pointer-events: none;
  cursor: auto !important;
  opacity: 0.3;
}
/* END General */
.af-view .af-class-collabs__tab.af-class-is--current > .af-class-collabs__tab-line{
	opacity: 1;
}
.af-view .af-class-collabs__tab.af-class-is--current > .af-class-collabs__tab-label{
	  color: #fff;
}
.af-view .af-class-navigation-section, .af-view .af-class-dot-scroll-section, .af-view .af-class-hero-black-overlay {
	pointer-events: none;
}
.af-view .af-class-navigation-section > * {
	pointer-events: auto;
}
@keyframes topLine {
  0% {
    transform: translate(0rem, 0rem) rotate(0deg);
    background-color: rgba(255,255,255,0.75);
  }
  100% {
    transform: translate(0rem, 3.6rem) rotate(45deg);
    background-color: rgba(0,0,0,1);
  }
}
@keyframes bottomLine {
  0% {
    transform: translate(0rem, 0rem) rotate(0deg);
		background-color: rgba(255,255,255,0.75);
  }
  100% {
    transform: translate(0rem, 0rem) rotate(-45deg);
		background-color: rgba(0,0,0,1);
  }
}
@keyframes returnTopLine {
  100% {
    transform: translate(0px, 0px) rotate(0deg);
  }
  0% {
    transform: translate(2px, 7px) rotate(45deg);
  }
}
@keyframes returnBottomLine {
  100% {
    transform: translate(0px, 0px) rotate(0deg);
  }
  0% {
    transform: translate(2px, -4px) rotate(-45deg);
  }
}
@media only screen and (min-width: 767px) {
	.af-view .af-class-collabs__scroll-img-wrap{
		animation: moveImagesFirst 21s linear 0s infinite normal forwards;
	}
	.af-view .af-class-collabs__scroll-img-wrap.af-class-is--2{
		animation: moveImagesSecond 21s linear 0s infinite normal forwards;
	}
  @keyframes moveImagesFirst {
  0% {
    transform: translate(0%, 0px);
  }
  100% {
    transform: translate(-200%, 0px);
  }
}
@keyframes moveImagesSecond {
  0% {
    transform: translate(-267%, 0px);
  }
  100% {
    transform: translate(0%, 0px);
  }
}
}
@media only screen and (max-width: 767px) {
	.af-view .af-class-collabs__scroll-img-wrap{
		animation: moveImagesFirst 21s linear 0s infinite normal forwards;
	}
	.af-view .af-class-collabs__scroll-img-wrap.af-class-is--2{
		animation: moveImagesSecond 21s linear 0s infinite normal forwards;
	}
  @keyframes moveImagesFirst {
  0% {
    transform: translate(0%, 0px);
  }
  100% {
    transform: translate(-300%, 0px);
  }
}
@keyframes moveImagesSecond {
  0% {
    transform: translate(-390%, 0px);
  }
  100% {
    transform: translate(0%, 0px);
  }
}
}
