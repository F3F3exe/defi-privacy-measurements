            window.location.assign('/cuties/sell');
        }

        function openTrello() {
            window.open('https://trello.com/b/CzVTAYQS/blockchain-cuties-public-roadmap');
        }

        function openBCU() {
            window.open('https://blockchaincutiesuniverse.com');
        }

        function openMobileMenu() {
            $('.landingMobHeader').toggleClass('fa-times');
            $('.landingMobOpen').toggleClass('landingMobClose');
            $('.landingHeader').toggleClass('landingHeaderMob');
        }

        function landingSubscribe() {
            const emailSuccessMsg = "Thank you for subscribing! <br> Please check your email and confirm your subscription there.";

            const email = $('#subscribe_email').val();

            if (emailIsValid(email)) {

                const subscribeBox = $('.landingSubscribeBox');
                subscribeBox
                    .addClass('active');

                const method = {
                    method: 'POST',
                    headers: {
                        'Accept': 'application/json',
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({email})
                };

                fetch('/rest/1/email_subscribe_presale', method)
                    .then((response) => response.json())
                    .then((response) => {
                        if (!response.error) {
                            setTimeout(function () {
                                $('.landingSubscribeDesc').html(emailSuccessMsg);

                                subscribeBox
                                    .addClass('done')
                                    .find('.landingSubscribeBoxBtn')
                                    .val('Success');
                            }, 500)
                        }
                    });
            }
        }

        function emailIsValid(email) {
            return /^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(email);
        }

        function showWhatVideo() {
            const videoFrame = '<iframe src="https://www.youtube.com/embed/G8uo2ATGQ3E" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>';
            $('.whatVideoPreview').replaceWith(videoFrame);
        }

        function loadMedium() {
            fetch('/rest/medium')
                .then((response) => response.json())
                .then((data) => {
                    const posts = data.posts;

                    const containers = $(".medium-line");

                    posts.forEach(post => {
                        let block = $($("#medium-post").html());
                        $("a", block).attr("href", post.url);
                        $(".blogTitle", block).text(post.title);
                        $(".blogDesc", block).text(post.description);
                        $(".blogDate", block).text(post.date);
                        $(".blogImg", block).css("background-image", "url(" + post.image + ")");
                        containers.append(block);
                    });

                    $('#checkBlogBox').slick({
                        variableWidth: true,
                        autoplay: true,
                        infinite: true
                    });
                });
        }

        function loadTelegram() {
            fetch('/rest/1/telegram/member_count')
                .then((response) => response.json())
                .then((data) => {
                    const groups = data.message;

                    groups.forEach(group => {
                        if (group.count) {
                            $('.teleStat_' + group.lang).html(group.count + ' ' + "members");
                        }
                    });
                });
        }

        function loadDiscord() {
            fetch('https://discordapp.com/api/v6/invite/C6KCEU7?with_counts=true')
                .then((response) => response.json())
                .then((data) => {
                    $('.discordStat').html(data.approximate_member_count + ' ' + "members");
                });
        }

        function hidePromoLine() {
            $('#promo_line_landing_main').remove();
            $('.bottom_panel').css('top', 0);

            let now = new Date();
            now.setTime(now.getTime() + (24*60*60*1000)); // 1 day
            let time = Math.floor(Date.now() / 1000);

            document.cookie = "promo_line_disable=" + time + ";path=/";
        }

    </script>


</body></html>
