        function cookieWarning(){
            $('.cookie-warning__button').on('click', function () {

                Cookies.set('GdprNoticeDisable', 'agree');
                $('.cookie-warning-wrapper').removeClass('cookie-warning-wrapper--show');
            });

            if (!Cookies.get('GdprNoticeDisable')) {
                $('.cookie-warning-wrapper').addClass('cookie-warning-wrapper--show');
            }
        }

        function changeLanguage() {
            let lang = $(this).data('lang');

            let now = new Date();
            now.setTime(now.getTime() + (30*24*60*60*1000)); // 30 days
            let expires = "expires="+ now.toUTCString();

            document.cookie = "lang=" + lang + ";" + expires + ";path=/";
            const search = new URLSearchParams(location.search);
            search.set('lang', lang);
            location.search = search.toString();
        }

        function openLogin() {
            window.location.assign("/login" + location.search);
        }

        function openWiki() {
            window.open('https://docs.blockchaincuties.com', '_blank');
