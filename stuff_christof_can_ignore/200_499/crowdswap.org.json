		const mprice2 = (data.prices[2][1]).toFixed(3);
		const mprice3 = (data.prices[3][1]).toFixed(3);
		const mprice4 = (data.prices[4][1]).toFixed(3);
		const mprice5 = (data.prices[5][1]).toFixed(3);
		const mprice6 = (data.prices[6][1]).toFixed(3);
		const mprice7 = (data.prices[7][1]).toFixed(3);
		const mprice8 = (data.prices[8][1]).toFixed(3);
		
        
		const mprices = new Array(
