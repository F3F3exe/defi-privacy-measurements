		let points = "35," + my0 + " 70," + my1 + " 105," + my2 + " 140," + my3 + " 175," + my4 + " 210," + my5 + " 245," + my6 + " 280," + my7 + " 315," + my8;
		document.getElementById('mxy').setAttribute("points", points); 
		let points2 = "35,300 36," + my0 + " 35," + my0 + " 70," + my1 + " 105," + my2 + " 140," + my3 + " 175," + my4 + " 210," + my5 + " 245," + my6 + " 280," + my7 + " 315," + my8 + " 315,300";
		document.getElementById('mxy-mgrad').setAttribute("points", points2); 
    
		function getY(arr, obj) {
			const minimum = Math.min(...arr);
			if (obj == minimum) {
				return 270;
			} 
			const maximum = Math.max(...arr);
			if (obj == maximum) {
				return 30;
			}
			const a = maximum - minimum;
			const b = obj - minimum;
			return 270 - (240 * b.toFixed(3) / a.toFixed(3));
		}
		
    })
