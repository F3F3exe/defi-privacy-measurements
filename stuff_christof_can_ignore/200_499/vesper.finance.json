<script type="text/javascript">
	
	/* Lock the nav down until the dom is ready */
	let productNav = document.getElementsByClassName('vidNav');
	for(var i = 0; i < productNav.length; i++) {
		productNav[i].style["pointer-events"] = "none";
	}
	/* Set initial state of the first video */
	document.getElementById('growVid').addEventListener('loadedmetadata', function() {
	  this.currentTime = 1.5;
	  this.muted = true;
	}, false);
	
	jQuery(window).on("load", function() {
		
		/*  VIDEO ANIMATIONS
		*  A featured block using navigation links to control a rotating laptop. 
		*  When clicked, each link triggers a series of video animations:
		*  1.  Starts the first video (currently being presented) at 1.5 seconds in and plays to the end
		*  2.  Upon completion, the first video is hidden and the second video 
	       	(attached to the link that was clicked) begins to play.
		*  The sauce is in using 'requestAnimationFrame' to listen to the video animations and
		*  control the behavior based on specific times.  Each video is 3 seconds long.
		*/
		
		let productNav = document.getElementsByClassName('vidNav');
		for(var i = 0; i < productNav.length; i++) {
			productNav[i].style["pointer-events"] = "auto";
		}
	
		/* Set up the video actions */
		let navs = document.getElementsByClassName('vidNav');  //grab our nav links
		
		/* We want to prevent clicking while the videos are playing */
		function killNav() { /* Lock the nav */
			let navs = document.getElementsByClassName('vidNav');
			for(var i = 0; i < navs.length; i++) {
				navs[i].style["pointer-events"] = "none";
			}
		}
		function reviveNav() { /* Unlock the nav */
			let navs = document.getElementsByClassName('vidNav');
			for(var i = 0; i < navs.length; i++) {
				navs[i].style["pointer-events"] = "auto";
			}
		}
		function getActive() { //Get the currently active nav link
			let active = document.querySelector('.product-tabs .nav-link.active').dataset.vid;
			return active;
		}
		function pauseVid(video) { // pause video
			video.pause(); 
		}
		function hideVid(video) { // hide video
			video.style.display = 'none'; 
		}
		function playStart(video) { //Starts the first video
			video.play();
			killNav(); //lock the nav
			const hiding = function() { //Check current time, hide when video is done (all of these are 3 seconds)
				if (video.currentTime >= 3) {
			  		finishVid = document.getElementById(getActive());  // Get the second video (using the link that was clicked and is now active)
					finishVid.style.display = 'inline';
			  		startSecondVideo(finishVid); //pass second video ID and start playing the second video
					hideVid(video);
					window.cancelAnimationFrame(request); // Reset the animation frame to 0
				} else {
			  		window.requestAnimationFrame(hiding)
				}
			}
			request = window.requestAnimationFrame(hiding)
		}
		function startSecondVideo(video) { // Reveals and Starts our second video
			//video.style.display = 'inline';  
			video.play();
			revive = setTimeout(function() { //unblock our nav after the video finishes playing
				reviveNav();
			}, 1500);
			const pausing = function() { //play the second video and pause halfway, revealing the new laptop screen
				if (video.currentTime >= 1.5) {
			  		pauseVid(video);
			  		console.log(request);
			  		window.cancelAnimationFrame(req);
			  	
				} else {
			  		window.requestAnimationFrame(pausing)
				}
			}
			req = window.requestAnimationFrame(pausing);
			current = getActive(); // reset the currently active link to start our process again
		}
		
		/* Initialize our videos - Set the Grow video to start at 1.5 seconds */
		document.getElementById('growVid').addEventListener('loadedmetadata', function() {
	  	this.currentTime = 1.5;
	  	this.muted = true;
		}, false);
		document.getElementById('earnVid').addEventListener('loadedmetadata', function() {
	  	this.muted = true;
		}, false);
		/* document.getElementById('synthVid').addEventListener('loadedmetadata', function() {
	  	this.muted = true;
		}, false); */
		document.getElementById('orbitVid').addEventListener('loadedmetadata', function() {
	  	this.muted = true;
		}, false);
		
		/* Set the initial active video */
		let current = getActive();
		
		const tabs = document.querySelectorAll('button.nav-link'); // Get all of our links
		
		tabs.forEach( function(tab) {
			tab.addEventListener('click', function(event) {
				startVid = document.getElementById(current); // grab our starting video (first half of rotation)
				playStart(startVid); // Start the first video once it's ready
			});
		});
		
	});
		
</script>


